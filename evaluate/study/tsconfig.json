{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "allowJs": true,
        "jsx": "react",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "emitDecoratorMetadata": true,
        //"noUnusedLocals": true,
        //"noUnusedParameters": true,
        "noImplicitReturns": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "noImplicitUseStrict": true
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}
